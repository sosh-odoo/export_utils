<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="shopify_transfer_log_list" model="ir.ui.view">
    <field name="name">shopify.transfer.log.list</field>
    <field name="model">shopify.transfer.log</field>
    <field name="arch" type="xml">
        <list string="Shopify Sync Log">
            <field name="name"/>
            <field name="sync_status"/>
            <field name="transfer_category"/>
            <field name="transfer_date"/>
        </list>
    </field>
</record>

<record id="shopify_transfer_log_action" model="ir.actions.act_window">
    <field name="name">Shopify Sync Log</field>
    <field name="res_model">shopify.transfer.log</field>
    <field name="view_mode">list</field>
</record>

<menuitem id="shopify_root" name="Shopify Sync Log" web_icon="data_fetcher_shopify, static/description/icon.png" action="shopify_transfer_log_action"></menuitem>
</odoo>