<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="transfer_log_list" model="ir.ui.view">
    <field name="name">transfer.log.list</field>
    <field name="model">transfer.log</field>
    <field name="arch" type="xml">
        <list string="Import Logs">
            <field name="name"/>
            <field name="sync_status"/>
            <field name="transfer_category"/>
            <field name="source"/>
            <field name="error_message"/>
            <field name="transfer_date"/>
        </list>
    </field>
</record>

<record id="transfer_log_action" model="ir.actions.act_window">
    <field name="name">Import Logs</field>
    <field name="res_model">transfer.log</field>
    <field name="view_mode">list</field>
</record>

<menuitem id="import_root" name="Import Log" web_icon="data_fetcher_base, static/description/icon.png" sequence="1" action="transfer_log_action"></menuitem>
</odoo>